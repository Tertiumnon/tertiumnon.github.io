{
  "name": "tertiumnon-github-io",
  "version": "1.7.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:gh": "ng build --output-path docs --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "lint": "eslint --ext .ts,.html .",
    "lint:fix": "eslint --ext .ts,.html . --fix",
    "release:hotfix:finish": "npm version patch && npm run build:gh && git add --all && git commit --amend -C HEAD && git push && git push --tags && git checkout develop && git rebase origin/master && git push && git checkout master",
    "release:minor:finish": "npm version minor && npm run build:gh && git add --all && git commit --amend -C HEAD && git push && git checkout master && git merge develop && git push && git push --tags && git checkout develop",
    "release:major:finish": "npm version major && npm run build:gh && git add --all && git commit --amend -C HEAD && git push && git checkout master && git merge develop && git push && git push --tags && git checkout develop"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.5",
    "@angular/cdk": "^16.2.4",
    "@angular/common": "^16.2.5",
    "@angular/compiler": "^16.2.5",
    "@angular/core": "^16.2.5",
    "@angular/forms": "^16.2.5",
    "@angular/platform-browser": "^16.2.5",
    "@angular/platform-browser-dynamic": "^16.2.5",
    "@angular/router": "^16.2.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "update": "^0.7.4",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1602.2",
    "@angular-devkit/build-angular": "^16.2.2",
    "@angular-devkit/core": "^16.2.2",
    "@angular-devkit/schematics": "^16.2.2",
    "@angular-eslint/builder": "16.1.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/schematics": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "^16.2.2",
    "@angular/compiler-cli": "^16.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^18.17.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.51.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  }
}
